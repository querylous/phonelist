<div class="col-xs-4">
  <table class="table table-condensed table-striped"> 
    <thead>
      <tr> 
        <td width="100">Name</td>
        <td width="40">#</td>
        <td width="70">Number</td>
      </tr>
    </thead>
    <tbody>
      <% numbers.each do |number| %> 
        <tr>  
          <td width="100"><%= number[:name].humanize %></p></td>
          <td width="40"><%= number[:emp_no] %></p></td>
          <td width="100"><%= number[:phone_no] %></p></td>
        </tr>
      <% end %> 
    </tbody>
    </thead>
  </table>
</div>
